{
  "name": "@ezpie/crazy",
  "version": "1.0.0-beta",
  "description": "Craziest css library to build web UI easily",
  "main": "src/crazy.css",
  "style": "src/crazy.css",
  "scripts": {
    "build:dev": "postcss src/crazy.css -d css --config postcss.config.js -w",
    "build:prod": "postcss css/crazy.css -u cssnano -o css/crazy.min.css --no-map"
  },
  "keywords": [
    "css",
    "library"
  ],
  "author": "ezpie",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.0",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-discard-comments": "^6.0.0",
    "postcss-import": "^15.1.0"
  },
  "homepage": "https://crazycss.vercel.app",
  "bugs": {
    "url": "https://github.com/ezpieco/crazycss/issue"
  }
}